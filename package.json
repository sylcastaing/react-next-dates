{
  "version": "0.2.0",
  "name": "react-next-dates",
  "description": "Simple and Customizable DatePicker, DateRangePicker and TimePicker for React.",
  "keywords": [
    "react",
    "typescript",
    "date",
    "date-picker",
    "datepicker",
    "date-range-picker",
    "daterangepicker",
    "date-time-picker",
    "datetimepicker",
    "calendar",
    "clock",
    "date-fns"
  ],
  "author": "Sylvain Castaing @sylcastaing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylcastaing/react-next-dates.git"
  },
  "bugs": "https://github.com/sylcastaing/react-next-dates/issues",
  "main": "dist/index.js",
  "module": "dist/react-next-dates.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests",
    "test-watch": "dts test --watch",
    "coverage": "dts test --coverage",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "version": "dts build && storybook-to-ghpages",
    "release": "np",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@popperjs/core": "2.11.4",
    "classnames": "2.3.1",
    "react-popper": "2.2.5"
  },
  "peerDependencies": {
    "date-fns": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@size-limit/preset-small-lib": "7.0.8",
    "@storybook/addon-docs": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.4.19",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.4.19",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.4.19",
    "@storybook/storybook-deployer": "2.8.11",
    "@storybook/theming": "6.4.19",
    "@tsconfig/create-react-app": "1.0.2",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.4",
    "css-loader": "5.2.7",
    "cssnano": "5.1.5",
    "date-fns": "2.28.0",
    "dts-cli": "1.4.0",
    "husky": "7.0.4",
    "node-sass": "7.0.1",
    "np": "7.6.1",
    "pinst": "3.0.0",
    "postcss": "8.4.12",
    "prettier": "2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.2",
    "sass-loader": "10.1.1",
    "size-limit": "7.0.8",
    "storybook-addon-sass-postcss": "0.1.0",
    "style-loader": "2.0.0",
    "tslib": "2.3.1",
    "typescript": "4.6.3"
  },
  "size-limit": [
    {
      "path": "dist/react-next-dates.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/react-next-dates.esm.js",
      "limit": "20 KB"
    }
  ],
  "jest": {
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=10"
  }
}
